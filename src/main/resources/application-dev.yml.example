    # redis 配置
    data:
        redis:
            # 地址
            host: localhost
            # 端口，默认为6379
            port: 6379
            # 数据库索引
            database: 0
            # 密码
            password: your_redis_password
            # 连接超时时间
            timeout: 10s
            lettuce:
                pool:
                    # 连接池中的最小空闲连接
                    min-idle: 0
                    # 连接池中的最大空闲连接
                    max-idle: 8                    # 连接池的最大数据库连接数
                    max-active: 8
                    # 连接池最大阻塞等待时间（使用负值表示没有限制）
                    max-wait: -1ms
    ai:
        dashscope:
            api-key: your_api_key
            chat:
                options:
                    model: qwen-plus
            # 读取超时时间（毫秒）
            read-timeout: 120000
# Swagger配置
swagger:
    # 是否开启swagger
    enabled: true
    # 请求前缀
    pathMapping: /dev-api
# 防止XSS攻击
xss:
    # 过滤开关
    enabled: false
    # 排除链接（个用逗号分隔）
    excludes: /system/notice
    # 匹配链接
    urlPatterns: /system/*,/monitor/*,/tool/*

    